# User App Firebase Configuration (for user auth, profiles, orders, etc)
VITE_FIREBASE_API_KEY=your_user_app_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-user-app.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-user-app
VITE_FIREBASE_STORAGE_BUCKET=your-user-app.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_user_app_messaging_sender_id
VITE_FIREBASE_APP_ID=your_user_app_id

# Admin App Firebase Configuration (for product data)
VITE_ADMIN_FIREBASE_API_KEY=your_admin_app_api_key_here
VITE_ADMIN_FIREBASE_AUTH_DOMAIN=your-admin-app.firebaseapp.com
VITE_ADMIN_FIREBASE_PROJECT_ID=your-admin-app
VITE_ADMIN_FIREBASE_STORAGE_BUCKET=your-admin-app.appspot.com
VITE_ADMIN_FIREBASE_MESSAGING_SENDER_ID=your_admin_app_messaging_sender_id
VITE_ADMIN_FIREBASE_APP_ID=your_admin_app_id

# Other environment variables
VITE_APP_NAME=ChickenViken User App

# Firebase Emulator Configuration (optional)
# VITE_USE_FIREBASE_EMULATORS=true
